{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "Article",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "excerpt": {
      "type": "text"
    },
    "featuredImage": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "writer": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::writer.writer",
      "inversedBy": "articles"
    },
    "tags": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tag.tag"
    },
    "premium": {
      "type": "boolean"
    },
    "metadata": {
      "type": "component",
      "repeatable": false,
      "component": "seo.meta-data"
    },
    "elements": {
      "type": "dynamiczone",
      "components": [
        "elements.content-text",
        "elements.photo",
        "elements.video-url"
      ]
    },
    "subtitle": {
      "type": "text",
      "required": true
    }
  }
}
